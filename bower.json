{
  "name": "catcierge-web",
  "dependencies": {
    "bootstrap": "~3.2.0",
    "fontawesome": "~4.2.0",
    "vis": "~3.7.0",
    "handlebars.js": "~2.0.0"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "less/mixins/*",
        "less/!(variables).less",
